# Required for S3-compatible access (MinIO, AWS S3, etc.)
S3_ENDPOINT_URL=http://localhost:9000   # MinIO (or omit for AWS S3)
S3_ACCESS_KEY=minio_username            # or AWS_ACCESS_KEY_ID
S3_SECRET_KEY=minio_password            # or AWS_SECRET_ACCESS_KEY

S3_BUCKET=lakehouse                     # Bucket name
S3_REGION=us-east-1                     # Needed for AWS (optional for MinIO)
S3_INGEST_PREFIX=raw                    # Landing directory for raw data
S3_STAGE_PREFIX=stage                   # Directory for staged data
S3_MARTS_PREFIX=marts                   # Directory for data marts

# Lakehouse DuckDB
DUCKDB_PATH=./local/lakehouse.duckdb    # DuckDB file if used locally
